[distutils]
index-servers =
    pypi
    testpypi

[pypi]
username = __token__
# Replace <YOUR_PYPI_TOKEN> with your actual PyPI token
password = <YOUR_PYPI_TOKEN>

[testpypi]
username = __token__
# Replace <YOUR_TEST_PYPI_TOKEN> with your actual TestPyPI token 
password = <YOUR_TEST_PYPI_TOKEN>
repository = https://test.pypi.org/legacy/ 